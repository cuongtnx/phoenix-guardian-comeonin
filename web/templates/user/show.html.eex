<h2>Show user</h2>

<ul>

  <li>
    <strong>First name:</strong>
    <%= @user.first_name %>
  </li>

  <li>
    <strong>Last name:</strong>
    <%= @user.last_name %>
  </li>

  <li>
    <strong>Email:</strong>
    <%= @user.email %>
  </li>

</ul>

<div>
  <%= link "Edit", to: user_path(@conn, :edit, @user), class: "btn btn-info" %>
  <%= link "Back", to: user_path(@conn, :index), class: "btn btn-info"%>
</div>
